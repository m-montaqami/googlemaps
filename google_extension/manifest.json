//manifest.json
{
	"manifest_version": 2,
    "name": "googlemaps_export",
    "version": "0.1",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":["content.js"]
		}
						],
	"browser_action":{
		"default_icon" : "icon.png",
		"default_popup":"popup.html"
					 },
	"permissions":[
		"activeTab",
		"tabs"
				  ]
}
